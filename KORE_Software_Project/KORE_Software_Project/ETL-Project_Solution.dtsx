        <DTS:Executable
          DTS:refId="Package\Data Cleaning\Date Check"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
          DTS:DTSID="{6BAA4B39-CD4A-4D20-97A1-28F0AE9F87BB}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:LocaleID="-1"
          DTS:ObjectName="Date Check"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="1">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{178F1BC6-7502-4A7A-B9EB-52CBF6CA0755}"
              SQLTask:SqlStatementSource="BEGIN TRY&#xA;BEGIN TRANSACTION;&#xA;&#xA;INSERT INTO errorLog.Users (UserID, FullName, Age, Email, RegistrationDate, LastLoginDate, PurchaseTotal, ErrorMessage)&#xA;SELECT &#xA;    UserID, &#xA;    FullName, &#xA;    Age, &#xA;    Email, &#xA;    RegistrationDate, &#xA;    LastLoginDate, &#xA;    PurchaseTotal, &#xA;    'Date inputted in Registration or LastLogin is not correctly entered'&#xA;FROM stg.Users&#xA;WHERE &#xA;    DATEDIFF(day, GETDATE(), LastLoginDate) &gt; 0 OR&#xA;    DATEDIFF(year, RegistrationDate, GETDATE()) &gt; 40 OR&#xA;    DATEDIFF(year, LastLoginDate, GETDATE()) &gt; 40 OR&#xA;    DATEDIFF(year, LastLoginDate, RegistrationDate) &gt; 0;&#xA;&#xA;-- Delete invalid rows from stg.Users&#xA;&#xA;DELETE FROM stg.Users&#xA;WHERE &#xA;    DATEDIFF(day, GETDATE(), LastLoginDate) &gt; 0 OR&#xA;    DATEDIFF(year, RegistrationDate, GETDATE()) &gt; 40 OR&#xA;    DATEDIFF(year, LastLoginDate, GETDATE()) &gt; 40 OR&#xA;    DATEDIFF(year, LastLoginDate, RegistrationDate) &gt; 0;&#xA;&#xA;COMMIT TRANSACTION;&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;ROLLBACK TRANSACTION;&#xA;THROW;&#xA;END CATCH&#xA;&#xA;GO" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
